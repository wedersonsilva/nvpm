// Meta    {

workspace NVPM Docs

  tab main
    *buff readme  : README.md
    buff nvpmdoc : doc/nvpm.txt
    buff help    : /usr/share/nvim/runtime/doc/helphelp.txt

  tab plugged
    buff Neomake : ~/.config/nvim/plugged/neomake/doc/neomake.txt
    buff cleverf : ~/.config/nvim/plugged/clever-f.vim/doc/clever_f.txt
    term folder  : ranger ~/.config/nvim/plugged

  *tab settings
    buff init.vim : ~/.config/nvim/init.vim
    buff oldshool : ~/.config/nvim/colors/oldschool.vim
    buff license  : LICENSE

// }
// Plugin  {

workspace NVPM Plugin

  tab Main
    buff plugin   : plugin/nvpm.vim
    buff tasks    : task.txt
    buff init.vim : ~/.config/nvim/init.vim

  tab Colors
    buff syntax : syntax/nvpm.vim
    buff proj   : .nvpm/proj/plugin

// }
// VimDocs {

*workspace Docs

  tab Syntax
    buff syntax : /usr/share/nvim/runtime/doc/syntax.txt
    buff usr_06 : /usr/share/nvim/runtime/doc/usr_06.txt
    buff usr_44 : /usr/share/nvim/runtime/doc/usr_44.txt

* tab VimLang
    buff visual  : /usr/share/nvim/runtime/doc/visual.txt
    buff eval    : /usr/share/nvim/runtime/doc/eval.txt
    buff map     : /usr/share/nvim/runtime/doc/map.txt
    buff windows : /usr/share/nvim/runtime/doc/windows.txt
    buff options : /usr/share/nvim/runtime/doc/options.txt

  tab Main
    term Ranger : ranger /usr/share/nvim/runtime/doc

//}
